<?xml version="1.0" encoding="UTF-8"?>

<program_style>
  <style name="naming_format"/>
  <style name="line_statement">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
  </style>
  <style name="newline">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="2"/>
      <property statistic="{1=4}"/>
    </rule>
    <rule>
      <context number="3"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="4"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="5"/>
      <property statistic="{2=1}"/>
    </rule>
    <rule>
      <context number="6"/>
      <property statistic="{2=1}"/>
    </rule>
    <rule>
      <context number="7"/>
      <property statistic="{2=2}"/>
    </rule>
  </style>
  <style name="if_else_body_order">
    <rule>
      <context number="1"/>
      <property statistic="{1=3}"/>
    </rule>
  </style>
  <style name="indention">
    <rule>
      <context number="1"/>
      <property statistic="{1=23}"/>
    </rule>
  </style>
  <style name="optional_brace">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="2"/>
      <property statistic="{1=2, 2=1}"/>
    </rule>
  </style>
  <style name="brace_format">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
  </style>
  <style name="declaration_layout"/>
  <style name="declaration_location"/>
  <style name="structure_preference">
    <rule>
      <context number="16"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="19"/>
      <property statistic="{0=1}"/>
    </rule>
    <rule>
      <context number="22"/>
      <property statistic="{1=1, 2=1}"/>
    </rule>
    <rule>
      <context number="23"/>
      <property statistic="{2=1}"/>
    </rule>
  </style>
  <style name="body_layout">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="2"/>
      <property statistic="{2=2}"/>
    </rule>
  </style>
  <style name="lineWrapping">
    <rule>
      <context number="1"/>
      <property statistic="{1=2}"/>
    </rule>
    <rule>
      <context number="2"/>
      <property statistic="{2=2}"/>
    </rule>
  </style>
</program_style>
